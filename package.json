{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db::container::create": "docker run --name postgres -e POSTGRES_PASSWORD=postgres -p 5432:5432 -d postgres",
    "db::migrate": "npx prisma migrate dev --name init",
    "setup": "db::container::create && db::migrate",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "4.3.1",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "prisma": "4.3.1",
    "typescript": "4.8.3"
  }
}
